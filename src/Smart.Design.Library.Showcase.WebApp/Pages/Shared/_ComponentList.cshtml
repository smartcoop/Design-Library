@functions {

    private string IsActive(string componentTitle)
    {
        if (ViewData.TryGetValue("ComponentTitle", out var temp))
        {
            return Equals(componentTitle, temp!) ? "c-styleguide-list__item--active" : string.Empty;
        }
        return string.Empty;
    }
}

